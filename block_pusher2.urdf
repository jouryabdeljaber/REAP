  <link name="obx">
  </link>
  <joint name="object_x" type="prismatic">
    <parent link="base"/>
    <child link="obx"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="1 0 0" />
  </joint>
  <link name="oby">
  </link>
  <joint name="object_y" type="prismatic">
    <parent link="obx"/>
    <child link="oby"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="object">
    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.01" />
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0.01" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
