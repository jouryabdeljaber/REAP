         <box size="0.4 0.4 0.4" />
      </geometry>
      <material name="bloo" />
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
         <box size="0.4 0.4 0.4" />
      </geometry>
    </collision>
  </link>
  <joint name="object_theta" type="continuous">
